<div class="span4">
  <dl class="dl-horizontal">
    <dt>
      <%= t('.uploaded_by') %>
    </dt>
    <dd>
      <%= @bgeigie_import.user.name_or_email %>
    </dd>
    <dt>
      <%= t('.filename') %>
    </dt>
    <dd>
      <%= @bgeigie_import.source.file.filename %>
    </dd>
    <dt>
      <%= t('.number_of_lines') %>
    </dt>
    <dd>
      <%= @bgeigie_import.lines_count %>
    </dd>
    <dt>
      <%= t('.number_of_measurements') %>
    </dt>
    <dd>
      <%= '-' unless @bgeigie_import.processed? %>
      <%= @bgeigie_import.bgeigie_logs.count if @bgeigie_import.processed? %>
    </dd>
  </dl>
</div>