window.Templates = {
<% root = Rails.root.join('app', 'views','js_templates', 'views') %>
<%- files = Dir[root.join('**', '*.html.*')] -%>
<%  files.each do |f| %>
<% dir = File.basename(File.dirname(f.to_s.gsub(root.to_s,''))) %>
<%  file = File.basename(f.to_s).gsub(/^([^\.]*)(\..*)/, '\1') %>
  "<%= dir%>/<%= file %>": <%= raw render(
      :template => "js_templates/views/#{dir}/#{file}").to_json -%>
      <%- if f != files.last -%>,<%- end -%>
<% end %>
}